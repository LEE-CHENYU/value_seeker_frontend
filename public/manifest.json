{
  "manifest_version": 3,
  "name": "Value Seeker", 
  "description": "A Chrome extension to seek values",
  "version": "1.0.0",
  "action": {
    "default_title": "Open Value Seeker"
  },
  "permissions": [
    "activeTab",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "minimum_chrome_version": "116",
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png", 
    "128": "logo192.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}