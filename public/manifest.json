{
  "manifest_version": 3,
  "name": "Value Seeker",
  "description": "A Chrome extension to seek values",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open Value Seeker"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "scripts": {
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build"
  }
}